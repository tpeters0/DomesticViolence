
<div id="link-icon">
  <%= link_to butterflies_path(@butterflies) do %>
  <%= image_tag ('butterfly-sm.png') %>
  <% end %>
</div>
<h2><%= @butterfly.title %></h2>


<div class="butterfly_image">
	<%= image_tag @butterfly.image_url.to_s %>
</div>

<div class="butterfly_actions">
	<!-- <a href="/butterflies/<%= @butterfly.id %>/edit">Edit</a> -->
	<%= button_to("Delete", butterfly_path(@butterfly), :method => :delete) %>
</div>
