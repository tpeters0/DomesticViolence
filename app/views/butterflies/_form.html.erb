<%= form_for @butterfly,      remote: true, :html => {:multipart => true} do |f| %>

  <p>
   <%= f.label                :title, "Name of Butterfly" %>
   <%= f.text_field           :title %>
  </p>



  <p>
   <%= f.hidden_field         :image, :value => id="canvascontent" %>
  </p>

  <p>
    <%= f.hidden_field        :user_id %>
  </p>
  <p>
   <%= f.submit %>
  </p>

<% end %>
