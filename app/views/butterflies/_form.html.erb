<%= form_for @butterfly, :html => {:multipart => true} do |f| %>

  <p>
   <%= f.label                :title, "Name of Butterfly" %>
   <%= f.text_field           :title %>
  </p>

  <p>
   <%= f.file_field           :image %>
  </p>

  <p>
    <%= f.hidden_field        :user_id %>
  </p>

  <p>
   <%= f.submit %>
  </p>

<% end %>
